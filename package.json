{
  "name": "vscode-dired",
  "displayName": "vscode-dired",
  "description": "dired(File Manager) for VSCode, like Emacs",
  "version": "0.0.1",
  "publisher": "rrudi",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/shirou/vscode-dired"
  },
  "bugs": {
    "url": "https://github.com/shirou/vscode-dired/issues"
  },
  "engines": {
    "vscode": "^1.8.0"
  },
  "categories": [
    "Other"
  ],
  "keywords": [
    "filer",
    "dired"
  ],
  "activationEvents": [
    "onCommand:extension.dired.open"
  ],
  "main": "./out/src/extension",
  "contributes": {
    "commands": [
      {
        "command": "extension.dired.open",
        "title": "Open dired buffer"
      }
    ],
    "keybindings": [
      {
        "key": "ctrl+x f",
        "command": "extension.dired.open",
        "when": "editorTextFocus && !inDebugRepl"
      },
      {
        "key": "enter",
        "command": "extension.dired.enter",
        "when": "dired.open"
      },
      {
        "key": "shift+=",
        "command": "extension.dired.createDir",
        "when": "dired.open"
      },
      {
        "key": "shift+r",
        "command": "extension.dired.rename",
        "when": "dired.open"
      },
      {
        "key": "shift+c",
        "command": "extension.dired.copy",
        "when": "dired.open"
      },
      {
        "key": "shift+b",
        "command": "extension.dired.goUpDir",
        "when": "dired.open"
      },
      {
        "key": "space",
        "command": "extension.dired.select",
        "when": "dired.open"
      },
      {
        "key": "q",
        "command": "extension.dired.close",
        "when": "dired.open"
      }
    ],
    "configuration": {
      "type": "object",
      "title": "VSCode Dired",
      "properties": {
        "dired.fixed_window": {
          "type": "boolean",
          "default": "true",
          "description": "Open same tab (not configurable currently)"
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "tsc -p ./",
    "compile": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install"
  },
  "devDependencies": {
    "typescript": "^2.0.10",
    "vscode": "^1.0.0",
    "mocha": "^2.3.3",
    "@types/node": "^6.0.40",
    "@types/mocha": "^2.2.32"
  },
  "dependencies": {
    "mkdirp": "^0.5.1",
    "stat-mode": "^0.2.2"
  },
  "preview": true
}
